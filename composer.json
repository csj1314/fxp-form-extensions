{
    "name" : "sonatra/form-extensions",
    "type": "symfony-bundle",
    "description": "Add new types for Symfony form",
    "keywords": ["datetime", "date", "time", "picker", "form", "hammerjs", "knob", "select2", "ajax"],
    "homepage": "https://github.com/sonatra/sonatra-form-extensions",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "require" : {
        "php" : ">=5.6.0",
        "symfony/serializer": "~3.0",
        "symfony/http-foundation": "~3.0",
        "symfony/property-access": "~3.0",
        "symfony/event-dispatcher": "~3.0",
        "symfony/routing": "~3.0",
        "symfony/form": "~3.0",
        "sonatra/ajax" : "1.0.*@dev"
    },
    "require-dev" : {
        "symfony/doctrine-bridge" : "~3.0",
        "doctrine/orm" : "~2.5"
    },
    "suggest": {
        "fxp/composer-asset-plugin": "For manage NPM/Bower dependencies with Composer",
        "bower-asset/select2": "The Select2 jquery plugin",
        "bower-asset/sonatra-jquery-datetime-picker": "The datetime picker jquery plugin"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Component\\FormExtensions\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0-dev"
        }
    }
}
