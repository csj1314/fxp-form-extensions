{
    "name" : "sonatra/form-extensions",
    "type": "symfony-bundle",
    "description": "Add new types for Symfony form",
    "keywords": ["datetime", "date", "time", "picker", "form", "hammerjs", "knob", "select2", "ajax"],
    "homepage": "https://github.com/sonatra/sonatra-form-extensions",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require" : {
        "php" : "^7.1.0",
        "symfony/serializer": "^3.4.0|^4.0.0",
        "symfony/http-foundation": "^3.4.0|^4.0.0",
        "symfony/property-access": "^3.4.0|^4.0.0",
        "symfony/event-dispatcher": "^3.4.0|^4.0.0",
        "symfony/routing": "^3.4.0|^4.0.0",
        "symfony/form": "^3.4.0|^4.0.0",
        "sonatra/ajax" : "^1.0.0"
    },
    "require-dev" : {
        "symfony/doctrine-bridge" : "^3.4.0|^4.0.0",
        "doctrine/orm" : "^2.6.0",
        "foxy/foxy": "^1.0.0"
    },
    "config": {
        "foxy": {
            "enabled": false
        }
    },
    "autoload": {
        "psr-4": { "Sonatra\\Component\\FormExtensions\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0-dev"
        }
    }
}
